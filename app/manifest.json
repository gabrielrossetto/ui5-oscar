{
  "sap.app": {
    "id": "oscar.challenge",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "Main",
          "target": "Main"
        },
        {
          "pattern": "awardForm",
          "name": "AwardForm",
          "target": "AwardForm"
        }
      ],
      "targets": {
        "Main": {
          "viewName": "Main",
          "viewPath": "oscar.challenge.view",
          "viewLevel": 1
        },
        "AwardForm": {
          "viewName": "AwardForm",
          "viewPath": "oscar.challenge.view",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "oscar.challenge.view.Main",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.84.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "dataSource": "mainService",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/oscar/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "componentPreload": false
  }
}
